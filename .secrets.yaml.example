development:
  # General
  # ---------------------------------------------------------------------------

  # SECURITY WARNING: keep the secret key used in production secret!
  # For generate new secret key call `get_random_secret_key` from
  # `django.core.management.utils`.
  SECRET_KEY: 'your_secret_key'

  # Social auth
  # ---------------------------------------------------------------------------

  # Google OAUTH2 credentials are used for auth users by their Google accounts.
  SOCIAL_AUTH_GOOGLE_OAUTH2_KEY: 'your_google_consumer_key'
  SOCIAL_AUTH_GOOGLE_OAUTH2_SECRET: 'your_google_consumer_secret'

  # Vk OAUTH2 credential are used for auth users by their Vkontakte accounts.
  SOCIAL_AUTH_VK_OAUTH2_KEY: 'your_vk_app_id'
  SOCIAL_AUTH_VK_OAUTH2_SECRET: 'your_google_secure_key'



production:
  # General
  # ---------------------------------------------------------------------------

  # SECURITY WARNING: keep the secret key used in production secret!
  # For generate new secret key call `get_random_secret_key` from
  # `django.core.management.utils`.
  SECRET_KEY: 'your_secret_key'
